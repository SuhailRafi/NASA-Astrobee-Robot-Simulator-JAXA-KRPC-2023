-- camera sample rate (30Hz)
rate = 15.0;

-- the max number of samples we'll try for features
num_samp = 1000;

-- the number of features sent per message
num_features = 50;

-- Delay between registration and features is 50ms
delay_features = 0.01;

-- Time taken from shutter to delivery of data
delay_camera = 0.0033;
